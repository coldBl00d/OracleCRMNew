    <DesignTime>
      <Attr Name="_isNotNull" Value="true"/>
      <Attr Name="_checkCondition" Value='"WORSE_CASE_REVENUE" IS NOT NULL'/>
      <Attr Name="_isCheck" Value="true"/>
      <Attr Name="_DBObjectName" Value="REVENUE_ITEMS_WORSE_CASE_REVEN"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="model.eo.ItemsForOpportunityEO.WorseCaseRevenue"/>
    </AttrArray>
  </Key>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
