    IsNotNull="true"
    Precision="18"
    Scale="0"
    ColumnName="PRODUCT_ID"
    SQLType="NUMERIC"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    TableName="REVENUE_ITEMS"
    PrimaryKey="true">
    <validation:PreDefinedValidationBean
      Name="ProductIdRule0"
      ResId="SELECT_A_PRODUCT"
      MessageFlavor="IGNORE_DETAILS"
      Description="Mandatory"
      ValName="Mandatory"
      Subtype="MANDATORY"/>
  </Attribute>
  <Attribute
