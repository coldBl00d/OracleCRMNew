      Name="WinProbabilityRule0"
      ResId="ENTER_A_VALID_PROBABILITY"
      MessageFlavor="IGNORE_DETAILS"
      Description="Mandatory"
      ValName="Mandatory"
      Subtype="MANDATORY"/>
