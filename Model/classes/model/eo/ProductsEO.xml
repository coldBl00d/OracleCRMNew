<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE Entity SYSTEM "jbo_03_01.dtd">
<!---->
<Entity
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ProductsEO"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  DBObjectType="table"
  DBObjectName="PRODUCTS"
  AliasName="ProductsEO"
  BindingStyle="OracleName"
  xmlns:validation="http://xmlns.oracle.com/adfm/validation">
  <Attribute
    Name="ProductCode"
    Precision="18"
    Scale="0"
    ColumnName="PRODUCT_CODE"
    SQLType="NUMERIC"
    Type="oracle.jbo.domain.DBSequence"
    ColumnType="NUMBER"
    TableName="PRODUCTS"
    PrimaryKey="true"
    IsUpdateable="while_insert"
    DefaultValue="@0"
    Domain="oracle.jbo.domain.DBSequence"
    RetrievedOnInsert="true"
    IsQueriable="false"/>
  <Attribute
    Name="ProductName"
    Precision="255"
    ColumnName="PRODUCT_NAME"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="PRODUCTS">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="255"/>
    </DesignTime>
    <validation:CompareValidationBean
      Name="ProductName_Null"
      OnAttribute="ProductName"
      OperandType="EXPR"
      Inverse="true"
      CompareType="EQUALTO"
      ResId="PLEASE_ENTER_A_NAME">
      <validation:TransientExpression
        Name="ValidationRuleScript"
        trustMode="untrusted"
        CodeSourceName="ProductsEORow"/>
    </validation:CompareValidationBean>
  </Attribute>
  <Attribute
    Name="ProductDescription"
    Precision="1000"
    ColumnName="PRODUCT_DESCRIPTION"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="PRODUCTS"
    IsQueriable="false">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1000"/>
    </DesignTime>
  </Attribute>
  <Attribute
    Name="ProductPrice"
    Precision="18"
    Scale="2"
    ColumnName="PRODUCT_PRICE"
    SQLType="NUMERIC"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    TableName="PRODUCTS">
    <validation:CompareValidationBean
      Name="ProductPriceRule"
      ResId="ENTER_A_PRICE_GREATER_THAN_ZERO"
      OnAttribute="ProductPrice"
      OperandType="LITERAL"
      Inverse="false"
      CompareType="GREATERTHAN"
      CompareValue="0"/>
  </Attribute>
  <Attribute
    Name="ProductCategory"
    Precision="18"
    Scale="0"
    ColumnName="PRODUCT_CATEGORY"
    SQLType="NUMERIC"
    Type="java.lang.Long"
    ColumnType="NUMBER"
    TableName="PRODUCTS"/>
  <Key
    Name="ProductsPk"
    PrimaryKey="true">
    <DesignTime>
      <Attr Name="_DBObjectName" Value="PRODUCTS_PK"/>
    </DesignTime>
    <AttrArray Name="Attributes">
      <Item Value="model.eo.ProductsEO.ProductCode"/>
    </AttrArray>
  </Key>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</Entity>
