    TableName="USERS">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.eo.AKUsersEO.EndDate_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
    <validation:CompareValidationBean
      Name="EndDateRule0"
      ResId="THE_END_DATE_IS_BEFORE_THE_START_DATE"
      Description="Check if > than start date"
      OnAttribute="EndDate"
      OperandType="JBO"
      Inverse="false"
      CompareType="GREATERTHANEQUALTO"
      CompareValue="model.uvo.AKUsersUVO.StartDate"/>
    <validation:CompareValidationBean
      Name="EndDateRule1"
      Description="Check with current date"
      OnAttribute="EndDate"
      OperandType="EXPR"
      Inverse="false"
      CompareType="GREATERTHANEQUALTO">
      <validation:TransientExpression
        Name="ValidationRuleScript"
        trustMode="untrusted"
        CodeSourceName="AKUsersEORow"/>
    </validation:CompareValidationBean>
  </Attribute>
  <Attribute
    Name="AddressId"
    Precision="18"
    Scale="0"
