    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="USERS"
    IsNotNull="true"
    IsQueriable="false">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="255"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.eo.AKUsersEO.UName_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
    <validation:PreDefinedValidationBean
      Name="UNameRule1"
      ResId="PLEASE_ENTER_A_VALID_VALUE_FOR_USERNAME_"
      Description="Mandatory"
      ValName="Mandatory"
      Subtype="MANDATORY"/>
    <validation:LengthValidationBean
      Name="UNameRule0"
      ResId="USERNAME_MUST_HAVE_ATLEAST_8_CHARACTERS_"
      Description="usernameLength"
      OnAttribute="UName"
      CompareType="GREATERTHANEQUALTO"
      DataType="CHARACTER"
      CompareLength="8"
      Inverse="false"/>
  </Attribute>
  <Attribute
    Name="UPassword"
    Precision="255"
    ColumnName="U_PASSWORD"
    SQLType="VARCHAR"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    TableName="USERS"
    IsNotNull="true"
    IsQueriable="false">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="255"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="model.eo.AKUsersEO.UPassword_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
    <validation:PreDefinedValidationBean
      Name="UPasswordRule1"
      ResId="PLEASE_ENTER_A_VALID_VALUE_FOR_PASSWORD"
      Description="Mandatory"
      ValName="Mandatory"
      Subtype="MANDATORY"/>
    <validation:LengthValidationBean
      Name="UPasswordRule0"
      ResId="PASSWORD_MUST_HAVE_ATLEAST_8_CHARACTERS_"
      Description="PsswordLength"
      OnAttribute="UPassword"
      CompareType="EQUALTO"
      DataType="CHARACTER"
      CompareLength="8"
      Inverse="false"/>
  </Attribute>
  <Attribute
