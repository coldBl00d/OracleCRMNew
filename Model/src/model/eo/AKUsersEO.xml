      Description="Mandatory"
      ValName="Mandatory"
      Subtype="MANDATORY"/>
  </Attribute>
  <Attribute
    Name="LastName"
    Precision="255"
    ColumnName="LAST_NAME"
    SQLType="VARCHAR"
