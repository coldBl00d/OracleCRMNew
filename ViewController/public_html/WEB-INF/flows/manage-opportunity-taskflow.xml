<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="manage-opportunity-taskflow">
    <default-activity>manage_opportunity</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>tabContext</name>
      <value>#{pageFlowScope.tabContext}</value>
      <class>com.ui.pattern.dynamicShell.TabContext</class>
      <required/>
    </input-parameter-definition>
    <view id="manage_opportunity">
      <page>/WEB-INF/fragments/manage_opportunity.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
