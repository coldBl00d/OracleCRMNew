<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="view-oppertunity-taskflow">
    <default-activity>ExecuteWithParams</default-activity>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <input-parameter-definition id="__2">
      <name>PointerId</name>
      <value>#{pageFlowScope.PointerId}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <input-parameter-definition id="__4">
      <name>tabContext</name>
      <value>#{pageFlowScope.tabContext}</value>
      <class>com.ui.pattern.dynamicShell.TabContext</class>
    </input-parameter-definition>
    <view id="view-oppertunity_ar">
      <page>/WEB-INF/fragments/view-oppertunity_ar.jsff</page>
    </view>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>view-oppertunity_ar</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
