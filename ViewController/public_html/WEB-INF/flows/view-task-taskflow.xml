<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="view-task-taskflow">
    <default-activity>SetCurrentRowWithKeyValue</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>TaskId</name>
      <value>#{pageFlowScope.TaskId}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <input-parameter-definition id="__2">
      <name>tabContext</name>
      <value>#{pageFlowScope.tabContext}</value>
      <class>com.ui.pattern.dynamicShell.TabContext</class>
    </input-parameter-definition>
    <method-call id="SetCurrentRowWithKeyValue">
      <method>#{bindings.setCurrentRowWithKeyValue.execute}</method>
      <outcome>
        <fixed-outcome>setCurrentRowWithKeyValue</fixed-outcome>
      </outcome>
    </method-call>
    <view id="Tasks">
      <page>/WEB-INF/fragments/Tasks.jsff</page>
    </view>
    <control-flow-rule id="__3">
      <from-activity-id>SetCurrentRowWithKeyValue</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>setCurrentRowWithKeyValue</from-outcome>
        <to-activity-id>Tasks</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
