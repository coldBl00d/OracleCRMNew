                                        <f:validator binding="#{bindings.MobileNumber.validator}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.WorkNumber.inputValue}" simple="true"
                                                  required="#{bindings.WorkNumber.hints.mandatory}"
                                                  columns="#{bindings.WorkNumber.hints.displayWidth}"
                                                  maximumLength="#{bindings.WorkNumber.hints.precision}"
                                                  shortDesc="#{bindings.WorkNumber.hints.tooltip}" id="it9"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it9}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.WorkNumber.validator}"/>
                                    </af:inputText>
                                    <h:outputLabel value="Title" id="ol5"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol5}"/>
                                    <af:inputText value="#{bindings.Title.inputValue}" simple="true"
                                                  required="#{bindings.Title.hints.mandatory}"
                                                  columns="#{bindings.Title.hints.displayWidth}"
                                                  maximumLength="#{bindings.Title.hints.precision}"
                                                  shortDesc="#{bindings.Title.hints.tooltip}" id="it4"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it4}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.Title.validator}"/>
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.Title.format}"/>
                                    </af:inputText>
                                    <h:outputLabel value="Department" id="ol6"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol6}"/>
                                    <af:inputText value="#{bindings.DepartmentName.inputValue}" simple="true"
                                                  required="#{bindings.DepartmentName.hints.mandatory}"
                                                  columns="#{bindings.DepartmentName.hints.displayWidth}"
                                                  maximumLength="#{bindings.DepartmentName.hints.precision}"
                                                  shortDesc="#{bindings.DepartmentName.hints.tooltip}" id="it5"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it5}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.DepartmentName.validator}"/>
                                    </af:inputText>
                                    <h:outputLabel value="Buying Role" id="ol7"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol7}"/>
