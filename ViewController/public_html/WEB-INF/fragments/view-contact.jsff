                                                        required="#{bindings.BuyingRole.hints.mandatory}"
                                                        shortDesc="#{bindings.BuyingRole.hints.tooltip}" id="soc1"
                                                        binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.soc1}">
                                        <f:selectItems value="#{bindings.BuyingRole.items}" id="si1"
                                                       binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.si1}"/>
                                        <f:validator binding="#{bindings.BuyingRole.validator}"/>
                                    </af:selectOneChoice>
                                    <h:outputLabel value="Influence" id="ol8"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol8}"/>
                                    <af:selectOneChoice value="#{bindings.InfluenceLevel.inputValue}"
                                                        required="#{bindings.InfluenceLevel.hints.mandatory}"
                                                        shortDesc="#{bindings.InfluenceLevel.hints.tooltip}" id="soc2"
                                                        binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.soc2}">
                                        <f:selectItems value="#{bindings.InfluenceLevel.items}" id="si2"
                                                       binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.si2}"/>
                                        <f:validator binding="#{bindings.InfluenceLevel.validator}"/>
                                    </af:selectOneChoice>
                                    <h:outputLabel value="Address" id="ol9"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol9}"/>
                                    <af:inputText value="#{bindings.AccountName.inputValue}" simple="true"
                                                  required="#{bindings.AccountName.hints.mandatory}" columns="20"
                                                  maximumLength="#{bindings.AccountName.hints.precision}"
                                                  shortDesc="#{bindings.AccountName.hints.tooltip}" id="it8"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it8}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.AccountName.validator}"/>
                                    </af:inputText>
