                                                  required="#{bindings.AccountName.hints.mandatory}"
                                                  columns="#{bindings.Email.hints.displayWidth}"
                                                  maximumLength="#{bindings.Email.hints.precision}"
                                                  shortDesc="#{bindings.Email.hints.tooltip}" id="it2"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it2}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.Email.validator}"/>
                                    </af:inputText>
                                    <h:outputLabel value="Phone" id="ol4"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol4}"/>
