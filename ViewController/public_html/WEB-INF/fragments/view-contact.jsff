                                    <h:outputLabel value="Name" id="ol1"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol1}"/>
                                    <af:inputText value="#{bindings.AccountName.inputValue}" simple="true"
                                                  required="#{bindings.AccountName.hints.mandatory}"
                                                  columns="20"
                                                  maximumLength="#{bindings.AccountName.hints.precision}"
                                                  shortDesc="#{bindings.AccountName.hints.tooltip}" id="it1"
                                                  binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.it1}"
                                                  editable="onAccess">
                                        <f:validator binding="#{bindings.AccountName.validator}"/>
                                    </af:inputText>
                                    <h:outputLabel value="Gender" id="ol2"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol2}"/>
                                    <af:selectOneChoice value="#{bindings.Gender.inputValue}"
                                                        required="#{bindings.Gender.hints.mandatory}"
                                                        shortDesc="#{bindings.Gender.hints.tooltip}" id="soc3"
                                                        binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.soc3}">
                                        <f:selectItems value="#{bindings.Gender.items}" id="si3"
                                                       binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.si3}"/>
                                        <f:validator binding="#{bindings.Gender.validator}"/>
                                    </af:selectOneChoice>
                                    <h:outputLabel value="Email" id="ol3"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.ol3}"/>
