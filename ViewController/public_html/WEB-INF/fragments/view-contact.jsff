                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.tab2}"/>
                                <af:showDetailItem id="tab3" text="Notes"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.tab3}"/>
                                <af:showDetailItem id="tab4" text="Interactions"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.tab4}"/>
                                <af:showDetailItem id="tab5" text="Appointments"
                                                   binding="#{backingBeanScope.backing_WEBINF_fragments_viewcontact.tab5}"/>
                            </af:panelTabbed>
                        </af:gridCell>
                    </af:gridRow>
                </af:panelGridLayout>
            </f:facet>
        </af:decorativeBox>
    </af:panelGroupLayout>
    <!--oracle-jdev-comment:auto-binding-backing-bean-name:backing_WEBINF_fragments_viewcontact-->
