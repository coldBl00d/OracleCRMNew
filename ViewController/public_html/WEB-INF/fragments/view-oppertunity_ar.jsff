                                                                      popupTitle="Search and Result Dialog"
                                                                      id="iclov1"
                                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.iclov1}"
                                                                      columns="22"/>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl13"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl13}">
                                        <af:inputComboboxListOfValues label="Sales Stage"
                                                                      popupTitle="Search and Select: #{bindings.SalesStage.hints.label}"
                                                                      id="salesStageId"
                                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.iclov2}"
                                                                      value="#{bindings.SalesStage.inputValue}"
                                                                      model="#{bindings.SalesStage.listOfValuesModel}"
                                                                      required="#{bindings.SalesStage.hints.mandatory}"
                                                                      columns="16"
                                                                      shortDesc="#{bindings.SalesStage.hints.tooltip}">
                                            <f:validator binding="#{bindings.SalesStage.validator}"/>
                                        </af:inputComboboxListOfValues>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl14"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl14}">
                                        <af:inputComboboxListOfValues label="Sale Channel"
