                                            <af:convertNumber groupingUsed="false"
                                                              pattern="#{bindings.EstimatedDuration.format}"/>
                                        </af:inputText>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl10"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl10}">
                                        <af:inputDate value="#{bindings.OpportunityCloseDate.inputValue}"
                                                      label="End Date"
                                                      required="#{bindings.OpportunityCloseDate.hints.mandatory}"
                                                      columns="17"
                                                      shortDesc="#{bindings.OpportunityCloseDate.hints.tooltip}"
                                                      id="id1"
                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.id1}">
                                            <f:validator binding="#{bindings.OpportunityCloseDate.validator}"/>
                                            <af:convertDateTime pattern="#{bindings.OpportunityCloseDate.format}"/>
                                        </af:inputDate>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl12"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl12}">
