                                    <af:panelGroupLayout id="pgl5"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl5}">
                                        <af:inputText label="Name" id="it3"
                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.it3}"
                                                      editable="onAccess" value="#{bindings.OpportunityName.inputValue}"
                                                      required="#{bindings.OpportunityName.hints.mandatory}"
                                                      columns="20"
                                                      maximumLength="#{bindings.OpportunityName.hints.precision}"
                                                      wrap="soft">
                                            <f:validator binding="#{bindings.OpportunityName.validator}"/>
                                        </af:inputText>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl4"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl4}">
                                        <af:inputText label="Probablility" id="it2"
                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.it2}"
                                                      editable="onAccess"
                                                      value="#{bindings.OpportunityWinProb.inputValue}"
                                                      required="#{bindings.OpportunityWinProb.hints.mandatory}"
                                                      columns="18"
                                                      maximumLength="#{bindings.OpportunityWinProb.hints.precision}">
                                            <f:validator binding="#{bindings.OpportunityWinProb.validator}"/>
                                            <af:convertNumber groupingUsed="false"
                                                              pattern="#{bindings.OpportunityWinProb.format}"/>
                                        </af:inputText>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl7"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl7}">
                                        <af:inputText label="Revenue" id="it4"
                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.it4}"
                                                      editable="onAccess" rows="1"
                                                      value="#{bindings.Revenue.inputValue}"
                                                      required="#{bindings.Revenue.hints.mandatory}"
                                                      columns="20"
                                                      maximumLength="#{bindings.Revenue.hints.precision}">
                                            <f:validator binding="#{bindings.Revenue.validator}"/>
                                            <af:convertNumber groupingUsed="false"
                                                              pattern="#{bindings.Revenue.format}"/>
                                        </af:inputText>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl8"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl8}">
