                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.it6}"
                                                      editable="onAccess"
                                                      columns="18"
                                                      value="#{bindings.WorstCaseRevenue.inputValue}"
                                                      required="#{bindings.WorstCaseRevenue.hints.mandatory}"
                                                      maximumLength="#{bindings.WorstCaseRevenue.hints.precision}">
