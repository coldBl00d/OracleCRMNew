                                                      required="#{bindings.BudgetAmount.hints.mandatory}"
                                                      columns="21"
                                                      maximumLength="#{bindings.BudgetAmount.hints.precision}">
                                            <f:validator binding="#{bindings.BudgetAmount.validator}"/>
                                            <af:convertNumber groupingUsed="false"
                                                              pattern="#{bindings.BudgetAmount.format}"/>
                                        </af:inputText>
                                    </af:panelGroupLayout>
                                    <af:panelGroupLayout id="pgl11"
                                                         binding="#{backingBeanScope.backing_viewoppertunity_ar.pgl11}">
                                        <af:inputText label="Duration" id="it8"
                                                      binding="#{backingBeanScope.backing_viewoppertunity_ar.it8}"
                                                      editable="onAccess"
                                                      value="#{bindings.EstimatedDuration.inputValue}"
                                                      required="#{bindings.EstimatedDuration.hints.mandatory}"
                                                      columns="20"
                                                      maximumLength="#{bindings.EstimatedDuration.hints.precision}">
