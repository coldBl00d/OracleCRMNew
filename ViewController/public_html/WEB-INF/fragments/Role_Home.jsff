                              rowBandingInterval="0" selectedRowKeys="#{bindings.RolesU1.collectionModel.selectedRow}"
                              selectionListener="#{bindings.RolesU1.collectionModel.makeCurrent}" rowSelection="single"
                              fetchSize="#{bindings.RolesU1.rangeSize}"
                              partialTriggers="::qryId1 ::p1 ::d1 ::d2 ::p4 ::d4" id="t2"
                              binding="#{backingBeanScope.backing_WEBINF_fragments_Role_Home.t2}"
                              columnStretching="last" width="373">
                        <af:column sortProperty="#{bindings.RolesU1.hints.RoleName.name}" sortable="true"
                                   headerText="#{bindings.RolesU1.hints.RoleName.label}" id="c1">
                            <af:outputText value="#{row.RoleName}"
                                           shortDesc="#{bindings.RolesU1.hints.RoleName.tooltip}" id="ot1"/>
                        </af:column>
                        <af:column sortProperty="#{bindings.RolesU1.hints.StartDate.name}" sortable="true"
                                   headerText="#{bindings.RolesU1.hints.StartDate.label}" id="c2">
                            <af:outputText value="#{row.StartDate}"
                                           shortDesc="#{bindings.RolesU1.hints.StartDate.tooltip}" id="ot2">
