                        <f:facet name="buttonBar"/>
                        <af:outputText value="Are you sure you want to delete this role?" id="ot4"
                                       binding="#{backingBeanScope.backing_WEBINF_fragments_Role_Home.ot4}"
                                       inlineStyle="font-size:large;"/>
                    </af:dialog>
                </af:popup>
                <af:popup childCreation="deferred" autoCancel="disabled" id="p4"
                          binding="#{backingBeanScope.backing_WEBINF_fragments_Role_Home.p4}"
