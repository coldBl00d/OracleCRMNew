                            <f:selectItems value="#{bindings.AllRoleUserPairUVO_AK1.items}" id="si1"
                                           binding="#{backingBeanScope.backing_WEBINF_fragments_Role_Home.si1}"/>
                            <f:validator binding="#{bindings.AllRoleUserPairUVO_AK1.validator}"/>
